.header {
        height 116px
        width 100% 
        position absolute
        left 0 
        top 0 
        background: url('../images/ic_bj.png') no-repeat
        background-size: cover
        .title-font{
            font-size: 18px;
            color: #fff
        }
        .title-img,.title-font{
            height 28px
            position: absolute
            left 50%
            top 25px
            transform: translateX(-50%)
        }
        .back {
            position relative
            width 100%
            height 116px
            cursor pointer
            button{
                width 52px
                height 52px
                position absolute 
                transform-origin: 0 50%;
                left 0 
                top 16px 
                margin 0
                padding 0
                background: url('../images/ic_activity_return.png') no-repeat
                background-size: cover

            }
            img {
                width: 52px
                height 52px
            }
        }
    }