.header {
        height 116px
        width 100% 
        position absolute
        left 0 
        top 0 
        background: url('../images/ic_bj.png') no-repeat
        background-size: cover
        .title-img{
            height 28px
            position: absolute
            left 50%
            top 25px
            transform: translateX(-50%)
        }
        .back {
            position relative
            width 100%
            height 116px
            img {
                width 52px
                height 52px
                position absolute 
                left 0 
                top 16px
            }
        }
    }